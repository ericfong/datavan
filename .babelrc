{
  "presets": [
    [
      "env",
      {
        "useBuiltIns": true,
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    "transform-export-extensions",
    "transform-class-properties",
    "transform-object-rest-spread",
    "lodash",
    "dev-expression"
  ],
  "env": {
    "es6": {
      "presets": [
        [
          "env",
          {
            "modules": false,
            "useBuiltIns": true,
            "targets": {
              "node": "current"
            }
          }
        ]
      ],
      "plugins": [
        "transform-export-extensions",
        "transform-class-properties",
        "transform-object-rest-spread",
        "lodash",
        "dev-expression"
      ],
    },
    "test": {
      "presets": [
        "react"
      ]
    }
  }
}
