{
  "scripts": {
    "build": "npm run build:cjs && npm run build:es6",
    "build:cjs": "BABEL_ENV=cjs babel src --out-dir lib --ignore '**/test/**,**.test.js,**.test.jsx'",
    "build:es6": "BABEL_ENV=es6 babel src --out-dir lib/es6 --ignore '**/test/**,**.test.js,**.test.jsx'",
    "prepublish": "npm test && npm run eslint && rm -rf ./lib && npm run build",
    "babel": "NODE_ENV=test babel-node",
    "start": "npm run build:es6 -- --watch",
    "benchmark": "BENCHMARK=1 NODE_PATH=src jest --config=jest-benchmark-config.json",
    "eslint": "eslint --ext jsx --ext js src",
    "test": "NODE_PATH=src jest"
  },
  "name": "datavan",
  "version": "4.1.3",
  "description": "Define your collections layer over redux state, with mixins to fetch, submit changes to your server",
  "main": "./lib/index.js",
  "browser": "./lib/es6/index.js",
  "module": "./lib/es6/index.js",
  "engines": {
    "node": ">= 8.9.4"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/bower_components/",
      "/lib/",
      "/tmp/"
    ]
  },
  "dependencies": {
    "create-react-context": "^0.2.2",
    "fast-stable-stringify": "^1.0.0",
    "immutability-helper": "^2.6.6",
    "lodash": "^4.17.10",
    "mingo": "2.2.2",
    "recompose": "^0.27.0",
    "search-text-tokenizer": "^2.1.1"
  },
  "peerDependencies": {
    "fbjs": "^0.8.0",
    "react": "^15.0.0 || ^16.0.0",
    "react-redux": "^5.0.0",
    "redux": "^3.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "7.0.0-beta.46",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.46",
    "@babel/plugin-proposal-export-default-from": "7.0.0-beta.46",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.0.0-beta.46",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.46",
    "@babel/plugin-proposal-optional-chaining": "7.0.0-beta.46",
    "@babel/plugin-proposal-pipeline-operator": "7.0.0-beta.46",
    "@babel/preset-env": "7.0.0-beta.46",
    "@babel/preset-react": "7.0.0-beta.46",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "8.2.3",
    "babel-jest": "23.0.0-alpha.0",
    "babel-plugin-lodash": "3.3.2",
    "delay": "2.0.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-dependencies": "2.4.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jest": "21.15.1",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-lodash": "2.7.0",
    "eslint-plugin-react": "7.7.0",
    "jest": "22.4.3",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.7",
    "redux": "4.0.0",
    "shelljs": "0.8.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericfong/datavan.git"
  },
  "keywords": [
    "database",
    "mongo",
    "collection",
    "react",
    "redux",
    "reactive",
    "ajax",
    "fetch",
    "sync",
    "async"
  ],
