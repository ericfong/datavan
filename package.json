{
  "scripts": {
    "clean-build": "rm -rf ./lib && npm run prepublish",
    "prepublish": "babel src --out-dir lib --ignore spec.js,test.js",
    "start": "npm run prepublish -- --watch",
    "mocha": "cross-env NODE_ENV=test mocha --recursive --compilers js:babel-core/register src/**/*.test.js",
    "test": "cross-env NODE_PATH=src jest"
  },
  "name": "datavan",
  "version": "1.0.3",
  "description": "Define your collections layer over redux state, with mixins to fetch, submit changes to your server",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "jsnext:main": "./lib/index.js",
  "directories": {
    "lib": "./lib"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/bower_components/",
      "/lib/",
      "/tmp/"
    ]
  },
  "moduleRoots": [
    "src"
  ],
  "dependencies": {
    "fast-stable-stringify": "^0.2.0",
    "immutability-helper": "^2.2.0",
    "js-cookie": "^2.1.4",
    "lodash": "^4.16.4",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "reselect": "^3.0.0",
    "search-text-tokenizer": "^2.1.1",
    "sift": "^3.3.5"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.11.6",
    "cross-env": "^4.0.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-lodash": "^2.4.2",
    "eslint-plugin-mocha": "^4.9.0",
    "eslint-plugin-react": "^6.10.3",
    "invariant": "^2.2.2",
    "jest": "^19.0.2",
    "jsdom": "^10.1.0",
    "mocha": "^3.3.0",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "should": "^11.2.1",
    "sinon": "^2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericfong/datavan.git"
  },
  "keywords": [
    "database",
    "collection",
    "react",
    "redux",
    "reactive",
    "ajax",
    "fetch",
    "http",
    "sync",
    "async"
  ],
  "author": "Eric Fong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ericfong/reget/issues"
  },
  "homepage": "https://github.com/ericfong/reget#readme"
}
