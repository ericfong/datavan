{
  "scripts": {
    "build": "npm run build:cjs && npm run build:es6",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib --ignore spec.js,test.js",
    "build:es6": "cross-env BABEL_ENV=es6 babel src --out-dir lib/es6 --ignore spec.js,test.js",
    "prepublish": "npm test && rm -rf ./lib && npm run build",
    "babel": "cross-env NODE_ENV=test babel-node",
    "start": "npm run build:es6 -- --watch",
    "benchmark": "cross-env BENCHMARK=1 NODE_PATH=src jest --config=jest-benchmark-config.json",
    "eslint": "eslint --ext jsx --ext js",
    "test": "cross-env NODE_PATH=src jest"
  },
  "name": "datavan",
  "version": "2.5.2",
  "description": "Define your collections layer over redux state, with mixins to fetch, submit changes to your server",
  "main": "./lib/index.js",
  "browser": "./lib/es6/index.js",
  "module": "./lib/es6/index.js",
  "engines": {
    "node": ">= 8.3.0"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/bower_components/",
      "/lib/",
      "/tmp/"
    ]
  },
  "dependencies": {
    "fast-stable-stringify": "1.0.0",
    "immutability-helper": "2.3.1",
    "js-cookie": "2.1.4",
    "lodash": "4.16.4",
    "mingo": "2.0.1",
    "prop-types": "15.5.10",
    "react-redux": "5.0.6",
    "recompose": "0.25.0",
    "redux": "3.7.2",
    "reselect": "3.0.1",
    "search-text-tokenizer": "2.1.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "20.0.3",
    "babel-plugin-dev-expression": "0.2.1",
    "babel-plugin-lodash": "3.2.11",
    "babel-plugin-module-resolver": "2.7.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-runtime": "6.26.0",
    "cross-env": "5.0.5",
    "delay": "2.0.0",
    "enzyme": "2.9.1",
    "eslint": "4.6.1",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-dependencies": "2.4.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jest": "21.0.2",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-lodash": "2.4.5",
    "eslint-plugin-mocha": "4.11.0",
    "eslint-plugin-react": "7.3.0",
    "invariant": "^2.2.2",
    "jest": "20.0.4",
    "jsdom": "11.2.0",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-hot-loader": "3.0.0-beta.6",
    "react-test-renderer": "15.6.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericfong/datavan.git"
  },
  "keywords": [
    "database",
    "collection",
    "react",
    "redux",
    "reactive",
    "ajax",
    "fetch",
    "http",
    "sync",
    "async"
  ],
  "author": "Eric Fong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ericfong/datavan/issues"
  },
  "homepage": "https://github.com/ericfong/datavan#readme"
}
