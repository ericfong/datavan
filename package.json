{
  "scripts": {
    "build": "npm run build:cjs && npm run build:es6",
    "build:cjs": "BABEL_ENV=cjs babel src --out-dir lib --ignore spec.js,test.js,test",
    "build:es6": "BABEL_ENV=es6 babel src --out-dir lib/es6 --ignore spec.js,test.js,test",
    "prepublish": "npm test && npm run eslint && rm -rf ./lib && npm run build",
    "babel": "NODE_ENV=test babel-node",
    "start": "npm run build:es6 -- --watch",
    "benchmark": "BENCHMARK=1 NODE_PATH=src jest --config=jest-benchmark-config.json",
    "eslint": "eslint --ext jsx --ext js src",
    "test": "NODE_PATH=src jest"
  },
  "name": "datavan",
  "version": "3.5.2",
  "description": "Define your collections layer over redux state, with mixins to fetch, submit changes to your server",
  "main": "./lib/index.js",
  "browser": "./lib/es6/index.js",
  "module": "./lib/es6/index.js",
  "engines": {
    "node": ">= 8.9.4"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/bower_components/",
      "/lib/",
      "/tmp/"
    ]
  },
  "dependencies": {
    "fast-stable-stringify": "^1.0.0",
    "immutability-helper": "^2.6.6",
    "lodash": "^4.17.5",
    "mingo": "2.2.0",
    "recompose": "^0.26.0",
    "search-text-tokenizer": "^2.1.1"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-redux": "^5.0.0",
    "redux": "^3.7.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "22.4.1",
    "babel-plugin-lodash": "3.3.2",
    "babel-plugin-module-resolver": "3.1.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "6.26.0",
    "delay": "2.0.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.18.2",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-dependencies": "2.4.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jest": "21.13.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-lodash": "2.6.1",
    "eslint-plugin-react": "7.7.0",
    "jest": "22.4.2",
    "jsdom": "11.6.2",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.7",
    "react-test-renderer": "16.2.0",
    "redux": "3.7.2",
    "shelljs": "0.8.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericfong/datavan.git"
  },
  "keywords": [
    "database",
    "collection",
    "react",
    "redux",
    "reactive",
    "ajax",
    "fetch",
    "http",
    "sync",
    "async"
  ],
  "author": "Eric Fong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ericfong/datavan/issues"
  },
  "homepage": "https://github.com/ericfong/datavan#readme"
}
