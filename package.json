{
  "scripts": {
    "build": "npm run build:production && npm run build:es6",
    "build:production": "cross-env BABEL_ENV=production babel src --out-dir lib --ignore spec.js,test.js",
    "build:es6": "cross-env BABEL_ENV=es6 babel src --out-dir lib/es6 --ignore spec.js,test.js",
    "prepublish": "npm test && rm -rf ./lib && npm run build",
    "babel": "babel-node",
    "start": "npm run build:es6 -- --watch",
    "test": "cross-env NODE_PATH=src jest"
  },
  "name": "datavan",
  "version": "1.10.11",
  "description": "Define your collections layer over redux state, with mixins to fetch, submit changes to your server",
  "main": "./lib/index.js",
  "browser": "./lib/es6/index.js",
  "module": "./lib/es6/index.js",
  "jest": {
    "setupTestFrameworkScriptFile": "./tool/jestSetupTestFramework.js",
    "testEnvironment": "node",
    "roots": ["<rootDir>/src"],
    "testPathIgnorePatterns": ["/node_modules/", "/bower_components/", "/lib/", "/tmp/"]
  },
  "dependencies": {
    "fast-stable-stringify": "^0.2.0",
    "immutability-helper": "^2.2.2",
    "js-cookie": "^2.1.4",
    "lodash": "^4.16.4",
    "prop-types": "15.5.10",
    "react-redux": "^5.0.5",
    "recompose": "0.23.5",
    "redux": "3.7.1",
    "reselect": "^3.0.1",
    "search-text-tokenizer": "^2.1.1",
    "sift": "3.3.11"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-lodash": "3.2.11",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "1.5.2",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.11.6",
    "cross-env": "5.0.1",
    "enzyme": "2.9.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "2.6.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "6.0.0",
    "eslint-plugin-lodash": "2.4.3",
    "eslint-plugin-mocha": "4.11.0",
    "eslint-plugin-react": "7.1.0",
    "invariant": "^2.2.2",
    "jest": "^20.0.4",
    "jsdom": "^11.0.0",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-hot-loader": "3.0.0-beta.6",
    "should": "^11.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericfong/datavan.git"
  },
  "keywords": ["database", "collection", "react", "redux", "reactive", "ajax", "fetch", "http", "sync", "async"],
  "author": "Eric Fong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ericfong/datavan/issues"
